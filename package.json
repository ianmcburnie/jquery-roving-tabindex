{
  "name": "jquery-roving-tabindex",
  "description": "jQuery collection plugin that implements one-dimensional roving tabindex keyboard navigation",
  "version": "0.9.1",
  "scripts": {
    "start": "karma start --autoWatch true --singleRun false",
    "test": "karma start --autoWatch false --singleRun true",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build",
    "build": "npm run jsdoc && npm run minify",
    "clean": "rimraf jsdoc test_reports *.min.js lint.txt",
    "fix": "eslint jquery.rovingtabindex.js --fix",
    "jsdoc": "jsdoc jquery.rovingtabindex.js -d jsdoc",
    "lint": "eslint jquery.rovingtabindex.js > lint.txt",
    "minify": "jsmin -o jquery.rovingtabindex.min.js jquery.rovingtabindex.js"
  },
  "pre-commit": "prepublish",
  "author": "Ian McBurnie <ianmcburnie@hotmail.com>",
  "homepage": "https://github.com/ianmcburnie/jquery-roving-tabindex",
  "bugs": {
    "url": "https://github.com/ianmcburnie/jquery-roving-tabindex/issues"
  },
  "keywords": [
    "jquery-plugin",
    "ecosystem:jquery",
    "ebay",
    "accessibility",
    "a11y"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ianmcburnie/jquery-roving-tabindex.git"
  },
  "license": "ISC",
  "devDependencies": {
    "coveralls": "^2",
    "eslint": "^3",
    "eslint-config-google": "~0.6",
    "jasmine-core": "^2",
    "jquery": ">=1.11",
    "jquery-linear-navigation": ">=0.0.4",
    "jquery-migrate": "^3",
    "jsdoc": "^3",
    "jsmin": "^1",
    "karma": "^1",
    "karma-coverage": "^1",
    "karma-html-reporter": "~0.2",
    "karma-jasmine": "^1",
    "karma-junit-reporter": "^1",
    "karma-phantomjs-launcher": "^1",
    "phantomjs-prebuilt": "^2",
    "pre-commit": "^1",
    "rimraf": "^2"
  },
  "peerDependencies": {
    "jquery": ">=1.11",
    "jquery-linear-navigation": ">=0.0.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
